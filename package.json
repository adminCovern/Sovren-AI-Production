{
  "name": "sovren-ai-command-center",
  "version": "1.0.0",
  "description": "SOVREN AI Executive Command Center - Neural War Terminal",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build && tsc --project tsconfig.server.json",
    "build:client": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "next start",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "jest --testPathPattern=performance",
    "test:security": "jest --testPathPattern=security",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "config": "node scripts/config-manager.js",
    "config:validate": "node scripts/config-manager.js validate",
    "config:audit": "node scripts/config-manager.js audit",
    "config:report": "node scripts/config-manager.js report",
    "config:health": "node scripts/config-manager.js health",
    "config:check-env": "node scripts/config-manager.js check-env",
    "config:generate-env": "node scripts/config-manager.js generate-env",
    "voice:install": "python3 src/scripts/install_b200_voice_models.py",
    "voice:test": "python3 src/scripts/b200_tts_synthesis.py --text 'Testing B200 voice synthesis' --output test_output.wav",
    "voice:build": "docker build -f docker/voice-synthesis.dockerfile -t sovren-voice-synthesis .",
    "voice:run": "docker run --gpus all -p 8080:8080 sovren-voice-synthesis",
    "dashboard:init": "ts-node src/scripts/initialize-b200-dashboard.ts",
    "dashboard:monitor": "npm run dashboard:init",
    "dashboard:test": "jest src/tests/dashboard/B200Dashboard.test.ts",
    "coordination:test": "jest src/tests/coordination/MultiExecutiveCoordination.test.ts",
    "coordination:optimize": "node -e \"console.log('Optimizing executive placement across B200 GPUs...')\"",
    "benchmark:test": "jest src/tests/benchmarking/B200PerformanceBenchmark.test.ts",
    "benchmark:run": "node -e \"console.log('Running B200 performance benchmarks...')\"",
    "benchmark:all": "node -e \"console.log('Running all benchmark suites...')\"",
    "autoscaling:test": "jest src/tests/autoscaling/B200AutoScaler.test.ts",
    "autoscaling:start": "node -e \"console.log('Starting B200 auto-scaling system...')\"",
    "autoscaling:monitor": "node -e \"console.log('Monitoring auto-scaling performance...')\"",
    "db:migrate": "node scripts/db-migrate.js",
    "db:migrate:prod": "NODE_ENV=production node scripts/db-migrate.js",
    "db:seed": "node scripts/db-seed.js",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@react-three/cannon": "^6.10.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.17.10",
    "@reduxjs/toolkit": "^1.9.7",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "autoprefixer": "^10.4.15",
    "axios": "^1.5.0",
    "bcrypt": "^6.0.0",
    "cannon-es": "^0.20.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "gsap": "^3.12.2",
    "immer": "^10.0.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.279.0",
    "modesl": "^1.2.1",
    "next": "^15.1.3",
    "postcss": "^8.4.29",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^8.1.3",
    "redis": "^5.7.0",
    "reflect-metadata": "^0.2.2",
    "sip.js": "^0.21.2",
    "socket.io-client": "^4.7.2",
    "stripe": "^18.4.0",
    "tailwindcss": "^3.3.3",
    "three": "^0.179.1",
    "uuid": "^10.0.0",
    "ws": "^8.14.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/nextjs": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/three": "^0.179.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "storybook": "^8.4.7",
    "ts-jest": "^29.4.1",
    "typescript": "^5.2.2"
  },
  "overrides": {
    "xml2js": "^0.6.2",
    "esbuild": "^0.25.8",
    "uuid": "^10.0.0",
    "three": "^0.179.1",
    "inflight": "npm:lru-cache@^11.0.2",
    "glob": "^11.0.0",
    "rimraf": "^6.0.1",
    "three-mesh-bvh": "^0.8.0",
    "eslint": "^9.17.0",
    "@humanwhocodes/config-array": "npm:@eslint/config-array@^0.19.1",
    "abab": false,
    "domexception": false
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
